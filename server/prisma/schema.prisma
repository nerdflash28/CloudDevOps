datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Note {
  id          Int     @id @default(autoincrement())
  email       String
  title       String
  description String
  status      Status  @default(DEFAULT)
}

enum Status {
  DEFAULT
  LOCKED
  DELETED
}